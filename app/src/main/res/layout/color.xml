<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frame_layout"
    android:background="@drawable/color_rounded_corners"
    android:elevation="@dimen/elevation_4dp"
    android:layout_width="@dimen/width_32dp"
    android:layout_height="@dimen/height_32dp"
    android:layout_margin="@dimen/padding_8dp">

    <ImageView
        android:id="@+id/image_view"
        android:layout_width="@dimen/width_24dp"
        android:layout_height="@dimen/width_24dp"
        android:layout_gravity="center"
        android:visibility="gone"
        app:srcCompat="@drawable/ic_round_check_24"
        tools:ignore="ContentDescription" />
</FrameLayout>